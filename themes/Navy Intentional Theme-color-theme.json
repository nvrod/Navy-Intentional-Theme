	"tokenColors": [
		{
			"settings": {
				"foreground": "#e6eee0"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#e6eee0"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#07fc0380",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#e0c814" 
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#f72553"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#e6eee0",
			}
		},
		{
			"name": "Number",
